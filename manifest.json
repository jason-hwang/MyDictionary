{
  "manifest_version": 2,
  "name": "My Dictionary",
  "version": "0.2",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
    "webview",
    "fullscreen", "overrideEscFullscreen",
    "storage", "notifications"
  ]
}
