{
  "manifest_version": 2,
  "name": "My Dictionary",
  "short_name":"My Dictionary",
  "version": "0.87",
  "minimum_chrome_version": "24",
  "icons": {
    "16": "images/icon_16.png",
    "128": "images/icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["scripts/main.js"]
    }
  },
  "permissions": [
    "webview",
    "fullscreen", "overrideEscFullscreen",
    "storage", "notifications",
    "alwaysOnTopWindows"
  ]
}
